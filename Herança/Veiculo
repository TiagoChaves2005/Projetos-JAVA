/**
 * Classe para demonstrar o principio da Herança em uma modelagem para 
 * automóveis
 * @author Tiago Moura Chaves
 */
public class Veiculo {
    String marca;
    String modelo;
    int ano;
    float preço;
    
    //Construtor da classe Veiculo 

    public Veiculo(String marca, String modelo, int ano, float preço) {
        this.marca = marca;
        this.modelo = modelo;
        this.ano = ano;
        this.preço = preço;
    }
    
    public void exibirDadosVeiculo(){
        System.out.println("Marca: " + marca);
        System.out.println("Modelo: " + modelo);
        System.out.println("Ano de Fabricação: " + ano);
        System.out.println("Preço: " + preço);
    }    
}//fim da classe
